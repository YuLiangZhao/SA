<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/sas_guide_page_containter_4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/guide_page_imageView_4"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/sa_ads_group_4"
        android:contentDescription="@string/app_name"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/app_go_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="40dp"
        android:gravity="center"
        android:text="@string/app_run"
        android:background="@drawable/text_view_white_corner_border_2dp"
        android:textColor="@color/white"
        android:textSize="22sp"
        android:padding="10dp" />
</RelativeLayout>
